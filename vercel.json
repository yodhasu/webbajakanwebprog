{
    "version": 2,
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.3.1" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
  
    "name": "dart",
  
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
  
    "routes": [
        {
